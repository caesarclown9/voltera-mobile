# Production Environment Variables
# Скопируйте в .env.production и заполните реальными значениями

# API Configuration
VITE_API_URL=https://ocpp.evpower.kg
VITE_USE_REAL_API=true

# Supabase Configuration (ОБЯЗАТЕЛЬНО!)
VITE_SUPABASE_URL=https://YOUR_PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# App Configuration
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=EvPower

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CRASH_REPORTING=true
VITE_ENABLE_DEBUG_LOGS=false

# Maps (если используется)
VITE_YANDEX_MAPS_API_KEY=your_yandex_maps_key_here

# Test Mode (ТОЛЬКО для тестирования, отключить в production!)
VITE_TEST_MODE=false
VITE_TEST_CLIENT_ID=test-client-123