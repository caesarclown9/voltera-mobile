{
  "name": "voltera-pwa",
  "private": true,
  "version": "1.1.2",
  "description": "Voltera - Мобильное приложение для управления зарядкой электромобилей в Кыргызстане. Находите ближайшие станции, начинайте зарядку по QR-коду, управляйте балансом и отслеживайте историю зарядок.",
  "author": "Voltera",
  "homepage": "https://voltera.kg",
  "repository": {
    "type": "git",
    "url": "https://github.com/caesarclown9/voltera-mobile.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run sync-version && tsc -b && vite build",
    "build:no-version": "tsc -b && vite build",
    "sync-version": "node scripts/sync-version.js",
    "typecheck": "tsc -p tsconfig.strict.json --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "generate:feature": "node scripts/generate-feature-graphic.cjs",
    "pre-release": "bash scripts/pre-release-check.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@capacitor-community/http": "^1.4.1",
    "@capacitor-mlkit/barcode-scanning": "^7.3.0",
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/core": "^7.4.4",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/network": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@pbe/react-yandex-maps": "^1.2.5",
    "@rollup/rollup-linux-x64-gnu": "^4.52.2",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/query-persist-client-core": "^5.87.4",
    "@tanstack/react-query": "^5.15.5",
    "@tanstack/react-query-persist-client": "^5.87.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.1",
    "capacitor-secure-storage-plugin": "^0.12.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^10.18.0",
    "html5-qrcode": "^2.3.8",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "idb": "^8.0.3",
    "idb-keyval": "^6.2.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.541.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.3.5",
    "react-leaflet-cluster": "^2.1.0",
    "react-router-dom": "^6.8.1",
    "uuid": "^13.0.0",
    "zod": "^3.23.8",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.4",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.5",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^6.4.1",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
